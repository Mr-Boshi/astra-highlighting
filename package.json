{
    "name": "astra",
    "displayName": "Astra-syn",
    "description": "Syntax highlighting for ASTRA code files",
    "version": "0.5.0",
    "publisher": "MrBoshi",
    "repository": {
        "type": "git",
        "url": "https://github.com/Mr-Boshi/astra-highlighting"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "astra-model",
                "aliases": [
                    "ASTRA model",
                    "astra-model"
                ],
                "extensions": [
                    ".asml",
                    "-asml"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "astra-exp",
                "aliases": [
                    "ASTRA exp",
                    "astra-exp"
                ],
                "extensions": [
                    ".asex",
                    "-asex"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "astra-model",
                "scopeName": "source.astramod",
                "path": "./syntaxes/astramod.tmGrammar.json"
            },
            {
                "language": "astra-exp",
                "scopeName": "source.astraexp",
                "path": "./syntaxes/astraexp.tmGrammar.json"
            }
        ]
    }
}
